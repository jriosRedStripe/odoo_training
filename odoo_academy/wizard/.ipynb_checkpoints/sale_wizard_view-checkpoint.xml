<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="acedemy_sale_wizard_form_view">
            <field name="name">academy.sale.wizard.form</field>
            <field name="model">academy.sale.wizard</field>
            <field name="arch" type="xml">
                <form string="Create Sale Order for Session">
                    <group>
                         <field name="session_id" />
                         <field name="session_students_ids" invisible="1"/>
                         <field name="students_ids" domain="[('id','in','session_students_ids')]" />
                    </group>
                </form>               
            </field>          
        </record>  
    </data>
</odoo>